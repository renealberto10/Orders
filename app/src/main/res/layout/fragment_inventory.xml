<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.inventory.InventoryFragment">

    <EditText
        android:id="@+id/edtbuscarinvetario"
        android:layout_width="325dp"
        android:layout_height="30dp"
        android:background="@drawable/rounded_corner"
        android:ems="18"
        android:gravity="center"
        android:hint="- Codigo o nombre de articulo"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.087" />

    <Button
        android:id="@+id/btnsavepedido"
        android:layout_width="230dp"
        android:layout_height="30dp"
        android:background="@drawable/rounded_corner"
        android:backgroundTint="#feb53d"
        android:text="Buscar Producto"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtbuscarinvetario"
        app:layout_constraintVertical_bias="0.032"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>